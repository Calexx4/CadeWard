{
  "version": 2,
  "builds": [
    { "src": "Mi pagina web/**/*.html", "use": "@vercel/static" },
    { "src": "Mi pagina web/**/*.css", "use": "@vercel/static" },
    { "src": "Mi pagina web/imagen/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "Mi pagina web/$1" }
  ]
}
